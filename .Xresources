!lVegita urxvt config

!----------colors----------

! colors based on collection/visibone-alt-2
! special
*.foreground:   #FBFBFB
*.background:   #232629
*.cursorColor:  #cccccc

! black
*.color0:       #414952
*.color8:       #626E7A

! red
*.color1:       #cc6699
*.color9:       #ff99cc

! green
*.color2:       #99cc66
*.color10:      #ccff99

! yellow
*.color3:       #cc9966
*.color11:      #ffcc99

! blue
*.color4:       #6699cc
*.color12:      #99ccff

! magenta
*.color5:       #9966cc
*.color13:      #cc99ff

! cyan
*.color6:       #66cc99
*.color14:      #99ffcc

! white
*.color7:       #909090
*.color15:      #ffffff

!----------urxvt----------

URxvt.font: xft:Inconsolata for powerline:size=13
URxvt.scrollBar: false

!----------rofi----------
rofi.color-enabled: true
rofi.color-window: #333333, #ccff99, #333333
rofi.color-normal: #333333, #fbfbfb, #333333, #666666, #ccff99
rofi.color-active: #333333, #99ccff, #333333, #666666, #ccff99
rofi.color-urgent: #333333, #ff99cc, #333333, #666666, #ccff99

rofi.fake-transparency:	true
rofi.lines:		9
rofi.bw:		0
rofi.opacity:		"10"
rofi.hide-scrollbar:	true
rofi.width:		15
